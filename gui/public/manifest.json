{
  "manifest_version": 3,
  "name": "BrainWave Focus",
  "version": "1.0",
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
      "*://*.youtube.com/*",
      "*://youtube.com/*"
    ],
      "js": ["content.js"]
    }
  ],

  "action": {
    "default_popup": "index.html"
  }
}
